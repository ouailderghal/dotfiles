[global_config]
[keybindings]
  zoom_in = <Primary><Shift>k
  zoom_out = <Primary><Shift>j
  zoom_in_all = <Primary>plus
  zoom_out_all = <Primary>minus
  split_horiz = <Shift><Alt>h
  split_vert = <Shift><Alt>v
  search = <Shift><Alt>s
[profiles]
  [[default]]
    background_color = "#ffffff"
    cursor_color = "#aaaaaa"
    font = Fira Code 12
    foreground_color = "#000000"
    use_custom_command = True
    custom_command = /bin/zsh
    use_system_font = False
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
